# This file is generated by smith-config, DO NOT EDIT
# If any options are added here, consider whether they should also get added to
# the initial config file (deploy/setup/var/local/dnsmasq.conf)

# Resolve all DNS requests to this device
#address=/#/<%= ip_address %>

interface=<%= interface_name %>
interface=usb0

# Disable DNS server, default gateway, and broadcast address options
dhcp-option=3
dhcp-option=6
dhcp-option=28

# Don't store DHCP leases on disk
leasefile-ro

# Disable DNS server
port=0

dhcp-range=<%= dhcp_range_start %>,<%= dhcp_range_end %>,12h
dhcp-range=192.168.7.1,192.168.7.1,12h
